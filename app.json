{
  "expo": {
    "name": "ElderSafe",
    "slug": "ElderSafe",
    "splash": {
      "image": "./assets/images/mysplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#722890"
    },
    "version": "1.1.8",
    "orientation": "portrait",
    "icon": "./assets/icon-gradient.png",
    "newArchEnabled": true,
    "userInterfaceStyle": "dark",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.rjmobile.eldersafe",
      "buildNumber": "1.1.8",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#722890"
      },
      "package": "com.bobbrown.eldersafe",
      "versionCode": 18
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1bc4a660-8de6-4b68-b891-f2d1ccf2d0d4"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ]
  }
}
